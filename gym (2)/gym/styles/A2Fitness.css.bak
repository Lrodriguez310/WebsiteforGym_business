/* Heading Styles */

 #logo:hover{
	 cursor: pointer;
	 box-shadow: 0 0 1px 1px rgba(0, 0, 0, 0.5);
 }




header {
	
	background: linear-gradient(120deg, #fdfbfb 0%, #ebedee 100%);
    border-radius: 0 0 85% 85% / 30%;  /* curvature at the bottom of header */
	text-shadow: 0px 1px 0px #333;    /* shadow to the text */
    padding-bottom: 5px;
    border-bottom: 3px solid black;
    font-family:'Oswald';
}

h1 a{
    text-align: center;
    font-size: 30px;
}
h1{
    text-align: center;
}
h3{
    color: white;
    font-size: 30px;
    text-align: center;
    background-color: black;
    margin-bottom: 5px;
}

a:visited{
  color: black;
  text-decoration:underline;
}

#classes{
    display: block;
    float: left;
    text-align: center;
    background-color: #FFA500;
    margin-left: 41%;
    margin-top: 40px;
    font-weight: bold;
    width: 200px;
    height: 60px;
    font-size: 20px;    

}


/* JOIN IMAGE related */
#joinImg{
  width: 100%;

}

#join{
    width: 13%;  /* made the icon smaller */
    position: fixed;
    right: 10%;
    top: 10%;
	  cursor: pointer;
    z-index: 2;
    
}




#join:hover{
    transform: scale(1.3) rotate(10deg);
    
    transition: linear .9s

}
hr{
  height: 2px;
}
