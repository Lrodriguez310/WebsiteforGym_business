/* Web Page Background Styles */

html{
    background: linear-gradient(#434343 0%, black 100%);
}


/* Head Styles */

 #logo{
    margin-left: 42%;
    margin-top: 15px;
	border: 1px solid #ddd; /* Gray border */
    border-radius: 4px;  /* Rounded border */
    padding: 5px; /* Some padding */
    width: 150px; /* Set a small width */
}

nav a{
    margin: 20px;
    font-size: 20px;
    text-decoration: none;
    color: black;
    
}

a:link {
    color: black;
    text-decoration: none;
}

a:hover {
    font-weight: bold;
	  font-family:monospace;
    text-decoration: none;
	
	
}

nav{
    font-family:'Oswald';
    text-align: center;
    margin-top: 10px;
    margin-bottom: 10px;
    
}

.mainHeading{
    font-family: 'limelight';
  
}

