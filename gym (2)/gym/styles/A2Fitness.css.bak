/* Body Styles */
body{
	
    background: linear-gradient(-180deg, rgba(255,255,255,0.50) -70%, rgba(0,0,0,0.50) 100%);
    margin: auto;
    width: 1200px;
    height: 1700px;
    
}

#quote{
  font-family:'Fondamento-Italic' ;
  font-size: 20px;
  margin-top: 10px;
  margin-bottom: 10px;
  font-weight: 700;
  color: white;
  text-align: center;
}

video {
   /* height: 560px;
	width: 980px;*/
